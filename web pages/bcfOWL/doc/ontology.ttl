#################################################################
#    Object Properties
#################################################################

###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasAssignedTo
:hasAssignedTo rdf:type owl:ObjectProperty ;
               rdfs:domain :Topic ;
               rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
               rdfs:label "The user to whom this topic is assigned to. Recommended to be in email format. The list of possible values are defined in the extension schema."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasAuthor
:hasAuthor rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Comment
                                       :ExtensionSchema
                                     )
                       ] ;
           rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
           rdfs:label "Comment author"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasBitmap
:hasBitmap rdf:type owl:ObjectProperty ;
           rdfs:domain :Viewpoint ;
           rdfs:range :Bitmap ;
           rdfs:label "Object Property, pointing to the Bitmap"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasCameraDirection
:hasCameraDirection rdf:type owl:ObjectProperty ;
                    rdfs:domain :Camera ;
                    rdfs:range <http://www.opengis.net/ont/geosparql#wktLiteral> .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasCameraUpVector
:hasCameraUpVector rdf:type owl:ObjectProperty ;
                   rdfs:domain :Camera ;
                   rdfs:range <http://www.opengis.net/ont/geosparql#wktLiteral> .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasCameraViewPoint
:hasCameraViewPoint rdf:type owl:ObjectProperty ;
                    rdfs:domain :Camera ;
                    rdfs:range <http://www.opengis.net/ont/geosparql#wktLiteral> .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasClippingPlanes
:hasClippingPlanes rdf:type owl:ObjectProperty ;
                   rdfs:domain :Viewpoint ;
                   rdfs:range :ClippingPlanes ;
                   rdfs:label "Object Property, pointing to the Clipping Planes"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasColoring
:hasColoring rdf:type owl:ObjectProperty ;
