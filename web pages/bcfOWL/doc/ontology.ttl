             rdfs:label "Topic priority. The list of possible values are defined in the extension schema."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasProject
:hasProject rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Comment
                                        :Topic
                                        :Viewpoint
                                      )
                        ] ;
            rdfs:range :Project ;
            rdfs:label "Reference to the corresponding Project"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasSelection
:hasSelection rdf:type owl:ObjectProperty ;
              rdfs:domain :Component ;
              rdfs:range :Selection ;
              rdfs:label "Object Property, pointing to the Selection class"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasStage
:hasStage rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :ExtensionSchema
                                      :Topic
                                    )
                      ] ;
          rdfs:range :Stage ;
          rdfs:label "Stage this topic is part of (Predefined list)."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasTopic
:hasTopic rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Comment
                                      :Viewpoint
                                    )
                      ] ;
          rdfs:range :Topic ;
          rdfs:label "A link to the relating comment"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasTopicModifiedAuthor
:hasTopicModifiedAuthor rdf:type owl:ObjectProperty ;
                        rdfs:domain :Topic ;
                        rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                        rdfs:label "User who modified the topic. Exists only when Topic has been modified after creation."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasTopicStatus
:hasTopicStatus rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :ExtensionSchema
                                            :Topic
                                          )
                            ] ;
                rdfs:range :TopicStatus ;
                rdfs:label "Type of the topic (Predefined list)"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasTopicType
:hasTopicType rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :ExtensionSchema
                                          :Topic
                                        )
                          ] ;
              rdfs:range :TopicType ;
              rdfs:label "Type of the topic (Predefined list)"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasViewpoint
:hasViewpoint rdf:type owl:ObjectProperty ;
              rdfs:domain :Comment ;
              rdfs:range :Viewpoint ;
              rdfs:label "Back reference to the viewpoint"@en ,
                         "The viewpoint "@en .


