:hasCommentModifiedAuthor rdf:type owl:ObjectProperty ;
                          rdfs:domain :Comment ;
                          rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                          rdfs:label "The author who modified the comment"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Exception
                                          :Selection
                                        )
                          ] ;
              rdfs:range :Component ;
              rdfs:label "Pointer to the Component"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasCreationAuthor
:hasCreationAuthor rdf:type owl:ObjectProperty ;
                   rdfs:domain :Topic ;
                   rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:label "User who created the topic."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasException
:hasException rdf:type owl:ObjectProperty ;
              rdfs:domain :Viewpoint ;
              rdfs:range :Exception ;
              rdfs:label "Object Property, pointing to the Exception"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasExtensionSchema
:hasExtensionSchema rdf:type owl:ObjectProperty ;
                    rdfs:domain :Project ;
                    rdfs:range :ExtensionSchema ;
                    rdfs:label "URI to the extension schema."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasIfcElement
:hasIfcElement rdf:type owl:ObjectProperty ;
               rdfs:domain :Component ;
               rdfs:range <https://w3id.org/bot#Element> ;
               rdfs:label "The link to the linked data Ifc Element."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasIfcGuid
:hasIfcGuid rdf:type owl:ObjectProperty ;
            rdfs:domain :Component ;
            rdfs:range xsd:stringe .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasLabels
:hasLabels rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :ExtensionSchema
                                       :Topic
                                     )
                       ] ;
           rdfs:range :Label ;
           rdfs:label "Tags for grouping Topics. The list of possible values are defined in the extension schema."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/hasLines
:hasLines rdf:type owl:ObjectProperty ;
