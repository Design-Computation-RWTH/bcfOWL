#################################################################
#    Classes
#################################################################

###  http://lbd.arch.rwth-aachen.de/bcfOWL/Bitmap
:Bitmap rdf:type owl:Class ;
        rdfs:label "A list of bitmaps can be used to add more information, for example, text in the visualization."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/Camera
:Camera rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCameraDirection ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCameraUpVector ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCameraViewPoint ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ;
        rdfs:label "Camera View"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/ClippingPlanes
:ClippingPlanes rdf:type owl:Class ;
                rdfs:label "ClippingPlanes can be used to define a subsection of a building model that is related to the topic. Each clipping plane is defined by Location and Direction."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/Coloring
:Coloring rdf:type owl:Class ;
          rdfs:label "The Coloring element lists colors and a list of associated components that should be displayed with the specified color when displaying a viewpoint. The color is given in ARGB format. Colors are represented as 6 or 8 hexadecimal digits. If 8 digits are present, the first two represent the alpha (transparency) channel. For example, 40E0D0 would be the color <span style=\"color:#40E0D0;\";>Turquoise. More information about the color format can be found on Wikipedia."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/Comment
:Comment rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasCommentModifiedAuthor ;
                           owl:minCardinality "0"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasViewpoint ;
                           owl:minCardinality "0"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasAuthor ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasProject ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasTopic ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasCommentModifiedAuthor ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasViewpoint ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasCommentModifiedDate ;
                           owl:minCardinality "0"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasCommentDate ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasCommentText ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasCommentModifiedDate ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ;
         rdfs:label "The markup file can contain comments related to the topic. Their purpose is to record discussion between different parties related to the topic. Comment has also the Guid attribute for identifying it uniquely."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/Component
:Component rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasIfcElement ;
                             owl:minCardinality "0"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasIfcGuid ;
                             owl:minCardinality "0"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasOriginatingSystem ;
                             owl:minCardinality "0"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasIfcElement ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasIfcGuid ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasOriginatingSystem ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasAuthoringToolId ;
                             owl:minCardinality "0"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasAuthoringToolId ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ;
           rdfs:label "A component that links to a building element." .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/Exception
:Exception rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasComponent ;
                             owl:minCardinality "0"^^xsd:nonNegativeInteger
                           ] ;
           rdfs:label "Components to hide/show determined by default_visibility."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/ExtensionSchema
:ExtensionSchema rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasAuthor ;
                                   owl:minCardinality "1"^^xsd:nonNegativeInteger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasLabels ;
                                   owl:minCardinality "0"^^xsd:nonNegativeInteger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasPriority ;
                                   owl:minCardinality "0"^^xsd:nonNegativeInteger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasStage ;
                                   owl:minCardinality "0"^^xsd:nonNegativeInteger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasTopicStatus ;
                                   owl:minCardinality "1"^^xsd:nonNegativeInteger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasTopicType ;
                                   owl:minCardinality "1"^^xsd:nonNegativeInteger
                                 ] ;
                 rdfs:label "The extension define the mapping of a BCF project."@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/Label
:Label rdf:type owl:Class ;
       rdfs:label "The label is for tagging the Issue to be able to make a better categorization"@en .


###  http://lbd.arch.rwth-aachen.de/bcfOWL/Lines
:Lines rdf:type owl:Class ;
       rdfs:label "Lines can be used to add markup in 3D. Each line is defined by three dimensional Start Point and End Point. Lines that have the same start and end points are to be considered points and may be displayed accordingly."@en .


